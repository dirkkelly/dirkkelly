<div class="row">
  <% data.likes.each do |like| %>
    <div class="col-xs-12 col-md-6 col-lg-4 m-t-1">
      <div class="card card-inverse" style="background-color: #333; border-color: #333;">
        <a href="<%= like[:url] %>" onclick="track('Clicked Like', 'Like - Image', { name: '<%= like[:name] %>' })">
          <img class="card-img-top" src="<%= like[:image_url] %>" alt="<%= like[:name] %>">
        </a>
        <div class="card-block clearfix">
          <h3 class="card-title"><%= like[:name] %></h3>
          <p class="card-text"><%= like[:description] %></p>
          <a href="<%= like[:url] %>" class="btn btn-primary pull-xs-right" onclick="track('Clicked Like', 'Like - Button', { name: '<%= like[:name] %>', term: '<%= like[:action] %>' })">
            <strong>
              <%= like[:action] %>&nbsp;&nbsp;
              <i class="fa fa-chevron-right"></i>
            </strong>
          </a>
        </div>
      </div>
    </div>
  <% end %>
</div>
